{
  "name": "camino-flux",
  "version": "0.5.0",
  "description": "Flux géographiques de Camino, le cadastre minier numérique ouvert",
  "main": "index.js",
  "scripts": {
    "build": "node ./worker/index.js",
    "dev": "onchange '**/*.js' -i -- npm run build",
    "start": "npm run build && node ./index.js",
    "commit": "npx git-cz",
    "release": "standard-version && git push --follow-tags origin master && conventional-github-releaser"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "node-fetch": "^2.3.0",
    "rimraf": "^2.6.3",
    "serve-index": "^1.9.1",
    "util": "^0.11.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "onchange": "^5.2.0",
    "standard-version": "^4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
